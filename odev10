---1)select c.city_id, c.city, c.country_id, o.country from public.city c left join public.country o on c.country_id = o.country_id


---2)select p.payment_id, c.first_name, c.last_name from public.customer c right join public.payment p on c.customer_id = p.customer_id

---3)select p.rental_id, c.first_name, c.last_name from public.customer c full join public.rental p on c.customer_id = p.customer_id
