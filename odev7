---1)select rating, count (*) from public.film group by rating 


---2)select replacement_cost, count (*) from public.film group by replacement_cost having count (*)>50 


---3)select store_id , count (customer_id) from  public.customer group by store_id


---4)select country_id ,  count (*) from public.city group by country_id order by  2 desc limit 1
