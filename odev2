---1) select * from public.film where replacement_cost between 12.99 and 16.99 and replacement_cost <> 16.99 order by replacement_cost 


---2)select * from public.actor where first_name in ('Penelope' , 'Nick' , 'Ed') order by first_name, last_name


---3)select * from public.film where rental_rate in ( 0.99 , 2.99 , 4.99 ) and replacement_cost in ( 12.99, 15.99, 28.99)
